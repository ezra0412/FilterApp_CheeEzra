                    app:layout_constraintStart_toStartOf="@+id/textView195"
                    app:layout_constraintTop_toBottomOf="@+id/textView194" />

                <TextView
                    android:id="@+id/textView193"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Changed FC-D Quantity"
                    app:layout_constraintStart_toStartOf="@+id/textView196"
                    app:layout_constraintTop_toBottomOf="@+id/textView195" />

                <EditText
                    android:id="@+id/textView194"
                    style="@style/edit_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="  Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView195"
                    app:layout_constraintStart_toStartOf="@+id/textView195"
                    app:layout_constraintTop_toBottomOf="@+id/textView191"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/textView195"
                    style="@style/edit_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="  Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView196"
                    app:layout_constraintStart_toStartOf="@+id/textView196"
                    app:layout_constraintTop_toBottomOf="@+id/textView192"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/textView196"
                    style="@style/edit_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="  Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/editText3"
                    app:layout_constraintStart_toStartOf="@+id/editText3"
                    app:layout_constraintTop_toBottomOf="@+id/textView193"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView197"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Service Price "
                    app:layout_constraintStart_toStartOf="@+id/editText3"
                    app:layout_constraintTop_toBottomOf="@+id/textView196" />

                <EditText
                    android:id="@+id/editText3"
                    style="@style/edit_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/editText"
                    app:layout_constraintStart_toStartOf="@+id/editText"
                    app:layout_constraintTop_toBottomOf="@+id/textView197"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView199"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="(Upload Image) "
                    android:textColor="@color/light_green"
                    android:textColorHighlight="@color/background"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView34"
                    app:layout_constraintEnd_toEndOf="@+id/imageView34"
                    app:layout_constraintStart_toStartOf="@+id/imageView34"
                    app:layout_constraintTop_toTopOf="@+id/imageView34" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>