<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".ServiceFilter">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar21"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/yellow"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView178"
        style="@style/top_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Service Filter"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/toolbar21"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar21"
        app:layout_constraintEnd_toStartOf="@+id/textView178"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar21">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/background">

                <ImageView
                    android:id="@+id/imageView34"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView181"
                    app:layout_constraintStart_toStartOf="@+id/textView181"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0"
                    app:srcCompat="@drawable/white_image" />

                <TextView
                    android:id="@+id/textView180"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:text="Changed Filter 1 "
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/textView181"
                    app:layout_constraintTop_toBottomOf="@+id/imageView34"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView181"
                    style="@style/tv_dropDown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="    Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView185"
                    app:layout_constraintStart_toStartOf="@+id/textView185"
                    app:layout_constraintTop_toBottomOf="@+id/textView180"
                    app:layout_constraintVertical_bias="0.0" />

                <Button
                    android:id="@+id/button11"
                    style="@style/button_long"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:layout_marginBottom="24dp"
                    android:text="Service"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/editText"
                    app:layout_constraintVertical_bias="0.0" />

                <EditText
                    android:id="@+id/editText"
                    style="@style/edit_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/et_medium_bg"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Name"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/button11"
                    app:layout_constraintStart_toStartOf="@+id/button11"
                    app:layout_constraintTop_toBottomOf="@+id/textView182"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView182"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Note"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/editText"
                    app:layout_constraintTop_toBottomOf="@+id/editText3"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView183"
                    style="@style/tv_dropDown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="    Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView189"
                    app:layout_constraintStart_toStartOf="@+id/textView189"
                    app:layout_constraintTop_toBottomOf="@+id/textView186"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView184"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Changed Filter 2 "
                    app:layout_constraintStart_toStartOf="@+id/textView185"
                    app:layout_constraintTop_toBottomOf="@+id/textView181" />

                <TextView
                    android:id="@+id/textView185"
                    style="@style/tv_dropDown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="    Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView183"
                    app:layout_constraintStart_toStartOf="@+id/textView183"
                    app:layout_constraintTop_toBottomOf="@+id/textView184"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView186"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Changed Filter 3 "
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView183"
                    app:layout_constraintTop_toBottomOf="@+id/textView185"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView187"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Changed Filter 4 "
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/textView189"
                    app:layout_constraintTop_toBottomOf="@+id/textView183"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView188"
                    style="@style/lable"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="Changed Filter 5 "
                    app:layout_constraintStart_toStartOf="@+id/textView190"
                    app:layout_constraintTop_toBottomOf="@+id/textView189" />

                <TextView
                    android:id="@+id/textView189"
                    style="@style/tv_dropDown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="    Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView190"
                    app:layout_constraintStart_toStartOf="@+id/textView190"
                    app:layout_constraintTop_toBottomOf="@+id/textView187"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView190"
                    style="@style/tv_dropDown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:hint="    Leave Blank If Non"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView194"
                    app:layout_constraintStart_toStartOf="@+id/textView194"
                    app:layout_constraintTop_toBottomOf="@+id/textView188"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView191"
